# Pipenv: create .venv in project directory
PIPENV_VENV_IN_PROJECT=1

# PDF Ingestion Example Configuration

# Path to the input PDF file (relative to project root)
PDF_PATH=assets/big.pdf

# Path to the output text file (defaults to PDF_PATH with .txt extension)
OUTPUT_PATH=

# Pipetree host for progress reporting
PIPETREE_HOST=https://pipetree.io

# API key for pipetree (leave empty to disable progress reporting)
PIPETREE_API_KEY=
